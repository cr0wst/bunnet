{
  "name": "bunnet",
  "version": "1.1.0",
  "description": "A simple RabbitMQ client for developers",
  "main": "./out/main/index.js",
  "author": "Steve Crow dev@bunnetapp.com",
  "homepage": "https://bunnetapp.com",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "format": "prettier --plugin prettier-plugin-svelte --write .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "svelte-check": "svelte-check --tsconfig ./tsconfig.json",
    "typecheck": "npm run typecheck:node && npm run svelte-check",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:mac-prerelease": "npm run build && electron-builder --mac --universal --config -c.mac.identity=null"
  },
  "dependencies": {
    "@cloudamqp/amqp-client": "^3.1.1",
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/utils": "^3.0.0",
    "@zerodevx/svelte-json-view": "^1.0.9",
    "axios": "^1.6.8",
    "electron-settings": "^4.0.2",
    "electron-updater": "^6.1.7"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@electron-toolkit/eslint-config-ts": "^1.0.1",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@sveltejs/vite-plugin-svelte": "^3.0.1",
    "@types/node": "^18.19.9",
    "autoprefixer": "^10.4.19",
    "electron": "^28.2.0",
    "electron-builder": "^24.9.1",
    "electron-vite": "^2.0.0",
    "moment": "^2.30.1",
    "postcss": "^8.4.38",
    "prettier": "^3.2.4",
    "prettier-plugin-svelte": "^3.1.2",
    "svelte": "^4.2.9",
    "svelte-check": "^3.6.3",
    "svelte-hero-icons": "^5.1.0",
    "tailwindcss": "^3.4.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "ulid": "^2.3.0",
    "vite": "^5.0.12"
  }
}
